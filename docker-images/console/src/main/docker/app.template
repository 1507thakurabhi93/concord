server {
	listen      8080;
	server_name _;
	root        /opt/concord/console/concord-console-${project.version};

	location / {
	}

	location ~ ^/(api|logs)/ {
	    proxy_pass          http://SERVER_ADDR:SERVER_PORT;
	    proxy_redirect      off;
	    proxy_set_header    Host $host;
	    proxy_set_header    X-Forwarded-For $remote_addr;
	}
}
